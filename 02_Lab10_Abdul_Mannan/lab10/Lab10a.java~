public class Lab10a {
   public static void main( String[] args) {
      BigNum b1;
      BigNum b2;
      BigNum b3;
      
      b1 = new BigNum();
      b2 = new BigNum("1000");
      b3 = new BigNum("1100");
      //b2.add(b3);
      
      System.out.println(b1);
      System.out.println(b2);
      System.out.println(b2.isLessThan(b3));
      
   
   
   
   }
}